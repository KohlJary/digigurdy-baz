{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/../../Arduino/libraries/**",
                "/Applications/Teensyduino.app/Contents/Java/hardware/teensy/avr/libraries/**",
                "/Applications/Teensyduino.app/Contents/Java/hardware/teensy/avr/cores/teensy4/**",
                "/Applications/Teensyduino.app/Contents/Java/hardware/teensy/avr/cores/teensy3/**",
                "/Applications/Teensyduino.app/Contents/Java/hardware/teensy/avr/cores/teensy/**",
                "/Applications/Teensyduino.app/Contents/Java/hardware/teensy/avr/cores/teensy3",
                "/Applications/Teensyduino.app/Contents/Java/hardware/teensy/avr/libraries/Bounce"
            ],
            "defines": [
                "USBCON",
                "ARDUINO_TEENSY41",
                "USB_MIDI_SERIAL",
                "ARDUINO"
